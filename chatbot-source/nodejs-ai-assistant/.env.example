# GetStream credentials - Replace with your actual values from Stream Dashboard
STREAM_API_KEY=your_stream_api_key
STREAM_API_SECRET=your_stream_api_secret

# AI API keys
OPENAI_API_KEY=your_openai_api_key
GEMINI_API_KEY=your_gemini_api_key

# Tavily API key
TAVILY_API_KEY=your_tavily_api_key

# SSL Configuration for Development (Remove or set to 1 in production!)
# This fixes "unable to get local issuer certificate" errors in development
NODE_TLS_REJECT_UNAUTHORIZED=0